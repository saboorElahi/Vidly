@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>
        MembershipType:@Model.MembershipType.MembershipName    
    </li>
    
        @if (Model.DateOfBirth != null)
        {
            <li>
                Birthdate: @Model.DateOfBirth
            </li>
        }
    
</ul>
